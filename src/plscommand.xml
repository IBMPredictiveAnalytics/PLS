  </Subcommand>
  <Subcommand Name="CRITERIA">
    <Parameter Name="LATENTFACTORS" ParameterType="Integer"/>
  </Subcommand>
  <Subcommand Name="MODEL" IsArbitrary="True"/>
</Command>
